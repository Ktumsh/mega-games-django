{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["build.sh"]
      }
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/manage.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.12"
    },
    "commands": {
      "build": "sh build.sh"
    }
  }
}
